<p style="color: green"><%= notice %></p>

<% content_for :title, "Users" %>

<h1>Users</h1>

<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", backstore_user_path(user) %>
    </p>
  <% end %>
</div>

<%= link_to "New user", new_backstore_user_path %>
